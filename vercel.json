{
  "name": "fleet-management-system",
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/bookings/reminders",
      "schedule": "0 9 * * 1-5"
    },
    {
      "path": "/api/admin/process-scheduled-messages",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/bookings/daily-brief",
      "schedule": "0 13 * * 1-5"
    }
  ]
}
